{
    "name": "gointegro/hateoas-bundle",
    "type": "symfony-bundle",
    "description": "GOintegro HATEOAS Bundle",
    "keywords": ["Doctrine", "HATEOAS", "REST", "API", "JSON-API"],
    "homepage": "http://www.gointegro.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Lorenzana",
            "email": "javier.lorenzana@gointegro.com"
        },
        {
            "name": "Adolfo Castro Menna",
            "email": "adolfo.casto@gointegro.com"
        },
        {
            "name": "GOintegro Devs",
            "homepage": "http://www.gointegro.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/skqr/php-raml-parser.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/skqr/ReverseRegex.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle": "*",
        "symfony/doctrine-bridge": "*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "*",
        "justinrainbow/json-schema": "*",
        "rhumsaa/uuid": "*",
        "skqr/php-raml-parser": "dev-master@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpdocumentor/phpdocumentor": "*",
        "codeception/codeception": "*",
        "skqr/reverse-regex": "dev-master@dev"
    },
    "autoload": {
        "psr-0": { "GoIntegro\\Bundle\\HateoasBundle": "" }
    },
    "target-dir": "GoIntegro/Bundle/HateoasBundle",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    }
}
